    {
      "name": "Taylor & Francis Online",
      "host_allowlist": ["www.tandfonline.com"],
      "search_url": "https://www.tandfonline.com/action/doSearch?AllField={query}",
      "journal_url": "https://www.tandfonline.com/"
    },
    {
      "name": "SpringerLink",
      "host_allowlist": ["link.springer.com"],
