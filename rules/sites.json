{
  "rule_name": "Popular",
  "sites": [
    {
      "name": "ACS Publications",
      "host_allowlist": ["pubs.acs.org"],
      "search_url": "https://pubs.acs.org/action/doSearch?AllField={query}",
      "journal_url": "https://pubs.acs.org/"
    },
    {
      "name": "Hindawi",
      "host_allowlist": ["www.hindawi.com"],
      "search_url": "https://www.hindawi.com/search/all/?q={query}",
